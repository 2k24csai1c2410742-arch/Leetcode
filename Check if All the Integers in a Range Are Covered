#include <vector>

class Solution {
public:
    bool isCovered(std::vector<std::vector<int>>& ranges, int left, int right) {
        std::vector<bool> covered(51, false);
        for (const auto& range : ranges) {
            for (int i = range[0]; i <= range[1]; ++i) {
                if (i <= 50) {
                    covered[i] = true;
                }
            }
        }
        for (int i = left; i <= right; ++i) {
            if (!covered[i]) {
                return false;
            }
        }
        return true;
    }
};
